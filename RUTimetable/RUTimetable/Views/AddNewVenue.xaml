<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="RUTimetable.AddNewVenue">
	<ContentPage.Content>
<Entry x:Name="venue"  PlaceholderColor="Black" FontSize="13" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" />
<Button BackgroundColor="White" x:Name="Get" Text="Use Current Location" Command="{Binding GetLocation}" CommandParameter="{Binding Source={x:Reference venue}, Path=Text}" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" />
</ContentPage.Content>
		<ContentPage.ToolbarItems>
  <ToolbarItem Name="Semester1" Activated="Semester1"  Text="Semester 1" Order="Secondary" Priority="0" />
  <ToolbarItem Name="Semester2" Activated="Semester2" Text="Semester 2"  Order="Secondary" Priority="0" />
	<ToolbarItem Name="Settings" Activated="Settings" Text="Settings"  Order="Secondary" Priority="0" />
 </ContentPage.ToolbarItems>
</ContentPage>
